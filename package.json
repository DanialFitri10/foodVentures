{
  "name": "foodventures",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "nyc": {
    "include": [
      "utils/UserUtil.js",
      "utils/ResourceUtil.js",
      "index.js",
      "public/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "text-summary"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "scripts": {
    "test": "nyc mocha tests/UiTests.test.js",
    "start": "node index.js",
    "clean": "npx nyc clean",
    "frontend-instrument": "nyc instrument .\\public\\js\\home.js .\\public\\instrumented\\home.js && copy .\\public\\home.html .\\public\\instrumented\\ && copy .\\public\\images .\\public\\instrumented\\ && copy .\\public\\css .\\public\\instrumented\\",
    "frontend-report": "nyc report --reporter html --reporter text -t coverage-frontend --report-dir coverage-frontend/summary"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "chromedriver": "^121.0.0",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "fs": "^0.0.1-security",
    "mocha": "^10.2.0",
    "nodemailer": "^6.9.8",
    "selenium-webdriver": "^4.17.0",
    "winston": "^3.11.0"
  }
}
