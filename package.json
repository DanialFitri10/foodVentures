{
  "name": "foodventures",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "nyc": {
    "include": [
      "public/home.html",
      "utils/ResourceUtil.js",
      "utils/UserUtil.js"
    ],
    "exclude": [],
    "reporter": [
      "text-summary"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "scripts": {
    "test": "nyc mocha tests/*test.js --timeout 10000 --exit",
    "start": "node index.js",
    "clean": "npx nyc clean",
    "frontend-instrument": "nyc instrument .\\public\\js .\\public\\instrumented\\js && copy .\\public\\*.html .\\public\\instrumented\\ && copy .\\public\\images .\\public\\instrumented\\ && copy .\\public\\css .\\public\\instrumented\\ && del .\\coverage-frontend\\*.json",
    "frontend-report": "nyc report --reporter html --reporter text -t coverage-frontend --report-dir coverage-frontend/summary"
  },
  "keywords": [],
  "devDependencies": {
    "chai-http": "^4.4.0",
    "nyc": "^15.1.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "chai": "^4.3.10",
    "chromedriver": "^120.0.2",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "mocha": "^10.2.0",
    "nodemailer": "^6.9.7",
    "selenium-webdriver": "^4.17.0"
  }
}
